{
  "name": "coreable",
  "version": "1.0.1",
  "description": "Coreable",
  "private": false,
  "main": "ecosystem.config.js",
  "engines": {
    "node": "12.x"
  },
  "scripts": {
    "dev:docz": "docz dev --src='./docs' --port=8081",
    "dev:api": "nodemon -e .js,.ts --watch api --exec \"NODE_ENV=development TS_NODE_PROJECT='./api/tsconfig.json' ts-node ./api/server.ts\"",
    "dev:web": "npm run start --prefix ./react/",
    "test:api": "rimraf ./.nyc_output && NODE_ENV=test TS_NODE_PROJECT='./api/tsconfig.spec.json' nyc mocha --recursive --timeout 10000 -r ts-node/register \"api/__tests__/**/*.spec.ts\" --exit",
    "test:web": "npm run test --prefix ./react/",
    "test:docz": "",
    "build:docz": "rimraf ./.docz && docz build --src='./docs' && mkdir ./dist && mv ./.docz/dist ./dist/docs",
    "build:api": "tsc -p ./api/tsconfig.json",
    "build:web": "npm run build --prefix ./react/ && mkdir ./dist && mv ./react/build ./dist/public",
    "serve:docz": "docz serve",
    "serve:api": "node ./dist/server.js",
    "serve:web": "",
    "pipeline:api": "npm install && NODE_ENV=pipeline TS_NODE_PROJECT='./api/tsconfig.spec.json' mocha --recursive --timeout 25000 -r ts-node/register \"api/__tests__/**/*.spec.ts\" --exit",
    "pipeline:web": "npm install --prefix ./react/ && npm run test --prefix ./react/",
    "deploy": "rimraf ./dist && npm run build:api && npm run build:docz && npm run build:web && cp app.yaml ./dist && cp package.json ./dist",
    "start": "NODE_ENV=production node ./server.js",
    "clean": "rimraf ./.docz && rimraf ./dist && rimraf ./.nyc_output"
  },
  "keywords": [],
  "contributors": [
    "Jake Walklate <jakewalklate@gmail.com> (https://svnty.dev/)",
    "Hans Eh <Hanslee358@gmail.com> (https://github.com/0fficialkj)",
    "Peter Binns <binnspeter@icloud.com>"
  ],
  "author": "Coreable",
  "license": "Copyright (c) 2020 Coreable",
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/bluebird": "^3.5.30",
    "@types/chai": "^4.2.11",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.4",
    "@types/faker": "^4.1.10",
    "@types/jsonwebtoken": "^8.3.8",
    "@types/lodash": "^4.14.149",
    "@types/md5": "^2.1.33",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.9.2",
    "@types/validator": "^13.0.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "create-react-app": "^3.4.1",
    "docz": "^2.3.1",
    "mocha": "^7.1.1",
    "nodemon": "^2.0.2",
    "nyc": "^15.0.1",
    "rimraf": "^3.0.2",
    "ts-node": "^8.7.0",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "bcrypt": "^4.0.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-graphql": "^0.9.0",
    "faker": "^4.1.0",
    "graphql": "^14.6.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "mysql2": "^2.1.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "sequelize": "^5.21.6"
  }
}
