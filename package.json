{
  "name": "coreable",
  "version": "1.0.3",
  "description": "Coreable",
  "private": false,
  "main": "server.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/coreable/coreable.git"
  },
  "engines": {
    "node": "12.x"
  },
  "scripts": {
    "dev:docz": "docz dev --src='./docs' --port=8081",
    "dev:api": "nodemon -e .js,.ts --watch api --exec \"NODE_ENV=development TS_NODE_PROJECT='./api/tsconfig.json' ts-node ./api/server.ts\"",
    "dev:web": "react-app-rewired start",
    "test:api": "rimraf ./.nyc_output && NODE_ENV=test TS_NODE_PROJECT='./api/tsconfig.spec.json' nyc mocha --recursive --timeout 100000 -r ts-node/register \"api/__tests__/**/*.spec.ts\" --exit",
    "test:web": "react-app-rewired test",
    "build:docz": "rimraf ./.docz && docz build --src='./docs' && mv ./.docz/dist ./dist/docs",
    "build:api": "tsc -p ./api/tsconfig.json",
    "build:web": "CI=false react-app-rewired build",
    "serve:docz": "docz serve",
    "serve:api": "node ./dist/server.js",
    "pipeline:api": "NODE_ENV=pipeline TS_NODE_PROJECT='./api/tsconfig.spec.json' mocha --recursive --timeout 25000 -r ts-node/register \"api/__tests__/**/*.spec.ts\" --exit",
    "pipeline:web": "CI=true react-app-rewired test",
    "deploy": "rimraf ./dist && rimraf ./.docz && npm run build:api && npm run build:web && cp app.yaml ./dist && cp package.json ./dist",
    "start": "NODE_ENV=production node ./server.js",
    "start:local": "NODE_ENV=production node ./dist/server.js",
    "clean": "rimraf ./.docz && rimraf ./dist && rimraf ./.nyc_output"
  },
  "keywords": [],
  "contributors": [
    "Jake Walklate <jakewalklate@gmail.com> (https://svnty.dev/)",
    "Hans Eh <Hanslee358@gmail.com> (https://github.com/0fficialkj)",
    "Peter Binns <binnspeter@icloud.com>"
  ],
  "author": "Coreable",
  "license": "SEE LICENSE IN LICENSE.md",
  "devDependencies": {
    "@testing-library/jest-dom": "^5.5.0",
    "@testing-library/react": "^10.0.2",
    "@testing-library/user-event": "^12.0.11",
    "@types/bcrypt": "^3.0.0",
    "@types/bluebird": "^3.5.30",
    "@types/chai": "^4.2.11",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/faker": "^4.1.10",
    "@types/jsonwebtoken": "^8.3.8",
    "@types/lodash": "^4.14.149",
    "@types/md5": "^2.1.33",
    "@types/mocha": "^8.0.0",
    "@types/node": "^14.0.14",
    "@types/validator": "^13.0.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "docz": "^2.3.1",
    "mocha": "^8.0.1",
    "nodemon": "^2.0.3",
    "nyc": "^15.0.1",
    "rimraf": "^3.0.2",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "@material-ui/core": "^4.9.11",
    "bcrypt": "^5.0.0",
    "chart.js": "^2.9.3",
    "clsx": "^1.1.0",
    "cors": "^2.8.5",
    "create-react-app": "^3.4.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-graphql": "^0.11.0",
    "faker": "^4.1.0",
    "graphql": "^15.3.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "mysql2": "^2.1.0",
    "node-sass": "^4.13.1",
    "react": "^16.13.1",
    "react-app-rewired": "^2.1.5",
    "react-chartjs-2": "^2.9.0",
    "react-dom": "^16.13.1",
    "react-ga": "^3.0.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.4.1",
    "rxjs": "^6.5.5",
    "sequelize": "^6.2.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
